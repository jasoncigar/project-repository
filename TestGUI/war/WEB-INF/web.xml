<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>ImageViewer.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>TestForServer</servlet-name>
		<servlet-class>com.org.test.myproject.server.TestForServerImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestForServer</servlet-name>
		<url-pattern>/com.org.test.myproject.ImageViewer/TestForServer</url-pattern>
	</servlet-mapping>

</web-app>
